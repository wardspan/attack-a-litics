{
  "version": 2,
  "name": "attack-a-litics",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://attack-a-litics-backend.railway.app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_BACKEND_URL": "https://attack-a-litics-backend.railway.app",
    "NEXT_TELEMETRY_DISABLED": "1"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_BACKEND_URL": "https://attack-a-litics-backend.railway.app",
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  }
}